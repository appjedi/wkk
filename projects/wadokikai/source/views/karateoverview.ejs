<div class='row p-2' >
    <h1 class='float'><%=OVmain.title%></h1>
</div>  


<% if (OVsection1) { %>
  <div class="row p-2">

    <div class='col col-12' >
        <h3><%=OVsection1.title%></h3>
    </div>

    <div class='col col-12' >
      <% for (let i = 0; i < OVsection1.paras.length; i += 1) { %>
      <p><%=OVsection1.paras[i].para%></p>
      <% } %>
    </div>

  </div>

<% } %>

