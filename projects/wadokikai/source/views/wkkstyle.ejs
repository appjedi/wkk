<div class="row p-2">
  <div class='col col-12' >
    <h1 class=''><%=wkkmain.title%></h1>
  </div>  
</div>

<% if (wkksection1) { %>
  <div class="row p-2">

    <div class='col col-12' >
      <h3><%=wkksection1.title%></h3>
    </div>
  
    <div class='col col-6' >
      <% for (let i = 0; i < wkksection1.paras.length; i += 1) { %>
        <p><%=wkksection1.paras[i].para%></p>
        <% } %>
    </div>

    <div class='col col-6'>
        <% if (wkksection1Img) { %>
        <img src="<%=wkksection1Img%>" />
        <% } %>
    </div>

  </div>
<% } %>

